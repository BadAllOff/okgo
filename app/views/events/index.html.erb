<div class="row">
  <div class="col-md-10">

    <%= render 'events/info_boxes' %>

    <div class="box ">
      <div class="box-header with-border">
        <h3 class="box-title"><%= t('events.create_new_event') %></h3>
        <div class="box-tools pull-right">
          <!-- Buttons, labels, and many other things can be placed here! -->
          <%= link_to t('events.event.create_event'), new_event_path, class: 'btn btn-sm btn-primary' %>
        </div><!-- /.box-tools -->
      </div><!-- /.box-header -->
    </div><!-- /.box -->

  <!-- /.timeline-label -->
  <% @events.each do |event| %>
      <%= render partial: 'events/event', locals: {event: event}%>
  <% end %>

  </div>
</div>
