<ins class="adsbygoogle"
     style="display:block;margin-bottom:20px;"
     data-ad-client="ca-pub-5677662134676433"
     data-ad-slot="8674757998"
     data-ad-format="auto"></ins>
<script>
  (adsbygoogle = window.adsbygoogle || []).push({});
</script>

<div class="box">
  <div class="box-header">
    <%= link_to t('feedbacks.feedback.leave_feedback'), new_feedback_path, class: 'btn btn-primary btn-sm pull-right' %>
  </div>

  <div class="box-body">
    <ul class="timeline">
      <li class="time-label">
        <span class="bg-red">
          <%= I18n.l Time.zone.now, format: ("%A, %b %d") %>
        </span>
      </li>
      <% @feedbacks.each do |feedback| %>
          <li>
            <%= feedback_status(feedback.status).html_safe %>
            <div class="timeline-item">
              <%= render partial: 'feedback', locals: {feedback: feedback} %>
            </div>
          </li>
      <% end %>
    </ul>
  </div>
</div>
<%= paginate @feedbacks, window: 2 %>
